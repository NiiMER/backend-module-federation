{
  "name": "@module-federation/aegis",
  "version": "0.0.1-alpha.71",
  "main": "lib/",
  "module": "esm/",
  "repository": "git@github.com:module-federation/aegis.git",
  "author": "Zack Jackson <zackary.l.jackson@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "yarn pretty -- --write",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "babel --delete-dir-on-start -d esm/ src/ && cp src/index.js esm && cp src/cluster.js esm && cp src/auth.js esm",
    "build:cjs": "babel --delete-dir-on-start --env-name cjs -d lib/ src/ && cp src/index.js lib && cp src/cluster.js lib && cp src/auth.js esm",
    "prepublish": "yarn build",
    "test": "jest"
  },
  "peerDependencies": {
    "core-js": "^3"
  },
  "dependencies": {
    "@octokit/core": "^3.4.0",
    "@octokit/rest": "^18.5.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graceful-shutdown": "^1.1.3",
    "express-jwt": "^6.0.0",
    "import-fresh": "^3.3.0",
    "jwks-rsa": "^2.0.1",
    "mongodb": "^4.0.0",
    "mongodb-client-encryption": "^1.2.3",
    "nanoid": "^3.1.20",
    "regenerator-runtime": "^0.13.7",
    "serverless-aws-static-file-handler": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/node": "7.14.9",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.14.9",
    "@types/node": "14.17.6",
    "babel-loader": "8.2.2",
    "babel-preset-env": "1.7.0",
    "chalk": "4.1.2",
    "core-js": "3.16.0",
    "cross-var": "1.1.0",
    "esm": "3.2.25",
    "express-cli": "0.0.1",
    "jest": "^27.0.6",
    "mocha": "9.0.3",
    "nodemon": "2.0.12",
    "rimraf": "3.0.2",
    "split": "1.0.1",
    "webpack": "5.48.0",
    "webpack-cli": "4.7.2",
    "webpack-node-externals": "3.0.0"
  }
}
